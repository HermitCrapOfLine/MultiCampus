package practice.inter;

public class aPlayer implements Game {

	int score;
	
	@Override
	public void startGame() {
		System.out.println("게임을 켭니다");
	}

	@Override
	public void endGame() {
		System.out.println("게임을 끕니다.");
	}

	@Override
	public void getScore(int score) {
		this.score = score;
	}
}
