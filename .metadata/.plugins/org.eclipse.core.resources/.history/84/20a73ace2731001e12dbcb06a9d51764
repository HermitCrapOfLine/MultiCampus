package java_study.chapter05;

public class Practice {

	public static void main(String[] args) {

		String str = new String("Hello");    // 1번
		
		str = new String("World");           // 2번
		
		String str2 = new String("World");   // 3번
		
		str = str2;                          // 4번
		
		str = null;                          // 5번
	}

}
