package java_study.chapter06;

import java_study.chapter06.TV;

public class MyRoom {

	public static void main(String[] args) {

		Phone p1 = new Phone();
		p1.internet();
		p1.color = "검정";
		p1.shape = "폴더";

		Phone p2 = new Phone();
		p2.talkBykakao();
		p2.color = "빨강";
		p2.shape = "네모";

		System.out.println(p2.color);
		System.out.println(p2.shape);

		TV tv1 = new TV();
		tv1.turnOn();
		tv1.size = 50;
		tv1.price = 1000000;
		tv1.brand = "LG";
		tv1.store = "홈쇼핑";

	}

}
