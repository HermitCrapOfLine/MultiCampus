package java_study.chapter06;

public class TV {
	int size;
	int price;
	String brand;
	String store;

	static void turnOff() {
		System.out.println("전원을 껐습니다.");
	}

	static void turnOn() {
		System.out.println("전원을 켰습니다.");
	}

	static void changeChannel() {
		System.out.println("채널을 바꿨습니다.");
	}
}
