package java_study.chapter07.sec05;

public class BbsUse {

	public static void main(String[] args) {
		Bbs b1 = new Bbs(1, "java", "fun java", "park");
		Bbs b2 = new Bbs(2, "jsp", "fun jsp", "hong");
		Bbs b3 = new Bbs(3, "spring", "fun spring", "kim");
		
		
		Bbs[] bArr = new Bbs[3];
		
		for (int i = 0; i < 3; i++) {
			bArr[i] = new Bbs()
		}
	}

}
