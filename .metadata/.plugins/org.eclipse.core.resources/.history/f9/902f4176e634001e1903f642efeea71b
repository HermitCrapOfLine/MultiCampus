package java_study.chapter06.sec5;

public class Account {

	
		String name;
		String accountName;
		int balance;
		
		public void deposit() {
			System.out.println( name + "이 " + balance + "원을 입금하다.");
		}
		
		public void withdraw() {
			System.out.println( name + "이 " + accountName + "에서 입금하다.");
		}
	}

}
