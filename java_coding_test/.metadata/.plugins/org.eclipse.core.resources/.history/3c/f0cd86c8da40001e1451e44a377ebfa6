package baekjun;

import java.util.Scanner;


public class Main {
	
	
	public int solution(int n, int[] arr1) {
		int answer = 0;
		for (int i = 1; i < n; i++) {
			for (int j = i + 1; j < n; j++) {
				if(arr1[i] != 0) {
					if(arr1[j] % arr1[i] == 0) {
					arr1[j] = 0;
				}
				}
			} 
		}
		for (int i = 1; i < n; i++) {
			if(i != 0) answer++;
		}
		
		return answer;
	}
	
	public static void main(String[] args) throws Exception {
		Main T = new Main();
		Scanner kb = new Scanner(System.in);
		int n = kb.nextInt();
		int[] arr = new int[n];
		for (int i = 1; i < n; i++) {
			arr[i] = i + 1;
		}
		System.out.println(T.solution(n, arr));
		
		}
	}


