	public static void main(String[] args) throws Exception {
		Main T = new Main();
		Scanner kb = new Scanner(System.in);
		int n = kb.nextInt();
		int[] arr = new int[n];
		for (int i = 0; i < arr.length; i++) {
			String s = new StringBuilder(kb.next()).reverse().toString();
			arr[i] = Integer.parseInt(s);
		}
		
